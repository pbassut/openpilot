  <snippet name="engaged curvature yaw">
   <global>engage_delay = 5
last_bad_time = -engage_delay</global>
   <function>curvature = value / v3
pressed = v1
enabled = v2

if (pressed == 1 or enabled == 0) then
  last_bad_time = time
end

if (time > last_bad_time + engage_delay) then
  return curvature
else
  return 0
end</function>
