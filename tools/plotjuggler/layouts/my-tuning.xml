  <snippet name="engaged_accel_actuator">
   <global>engage_delay = 5
last_bad_time = -engage_delay</global>
   <function>accel = value
brake = v1
gas = v2
enabled = v3

if (brake ~= 0 or gas ~= 0 or enabled == 0) then
  last_bad_time = time
end

